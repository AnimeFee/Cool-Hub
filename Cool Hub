local blacklistedUsers = {
    "omega24119",
    }

-- Function to check if a user is blacklisted
local function isBlacklisted(username)
    for _, blacklistedUsername in ipairs(blacklistedUsers) do
        if username == blacklistedUsername then
            return true
        end
    end
    return false
end

-- Function to kick blacklisted users
local function kickIfBlacklisted(player)
    if isBlacklisted(player.Name) then
        print(player.Name .. " is blacklisted. Access denied.")
        player:Kick("You are not authorized to use this script.")
    end
end

-- Check existing players
for _, player in pairs(game.Players:GetPlayers()) do
    kickIfBlacklisted(player)
end

-- Kick blacklisted users upon joining
game.Players.PlayerAdded:Connect(function(player)
    kickIfBlacklisted(player)
end)

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Cool Hub", HidePremium = false, SaveConfig = true, IntroText = "Cool Hub"})

-- Notification
OrionLib:MakeNotification({
  Name = "Logged in!",
  Content = "You are logged in as " .. Player.Name .. ".",
  Image = "rbxassetid://4483345998",
  Time = 3
})

-- Values
getgenv().key = "hello"
getgenv().keyinput = "string"

-- Functions
function makescript()
  destroy()
end

function keynoti()
  OrionLib:MakeNotification({
    Name = "Correct Key",
    Content = "access granted",
    Image = "rbxassetid://4483345998",
    Time = 5
  })
end

function nokeynoti()
  OrionLib:MakeNotification({
    Name = "Incorrect Key",
    Content = "no access granted incorrect key",
    Image = "rbxassetid://4483345998",
    Time = 5
  })
end

function destroy()
  game:GetService("CoreGui").Orion:Destroy()
end

-- Tabs
local Tab = Window:MakeTab({
  Name = "Colowner",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})

local Discord = Tab:AddSection({Name="Join Our Discord To Get Key"});
local KeySys = Tab:AddSection({Name="Key System"});


-- Textbox
KeySys:AddTextbox({
  Name = "Enter key",
  Default = "Enter Key Here",
  TextDisappear = true,
  Callback = function(Value)
    getgenv().keyinput = Value
  end
})

-- Buttons
KeySys:AddButton({
  Name = "Check Key",
  Callback = function()
    if getgenv().keyinput == getgenv().key then
      makescript()
      keynoti()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/AnimeFee/scriptsfr/main/CoolHub", true))()
    else
      nokeynoti()
    end
  end
})

Discord:AddButton({
  Name = "Copy Discord Link",
  Callback = function()
      setclipboard("https://discord.gg/88dfBYQk")
      print("Discord link copied to clipboard")
  end    
})
